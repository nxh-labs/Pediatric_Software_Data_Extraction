            "variables": [
              "age_in_month",
              "clinical_edema"
            ]
          },
          "descritpion": "F-100 dilué en phase 1 pour les <6 mois ou <3 kg sans œdèmes.",
          "variableExplanation": {
            "age_in_month": "Âge en mois.",
            "clinical_edema": "Présence d'œdèmes (True/False)."
          }
        },
        "code": "f100_diluted",
        "type": "nutritional",
        "duration": {
          "type": "while_in_phase"
        }
      },
      {
        "applicabilityCondition": {
