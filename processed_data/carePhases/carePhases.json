[
  {
    "code": "cnt_phase_aiguë",
    "name": "Phase 1 (Aiguë) du traitement au CNT",
    "description": "Stabilisation métabolique du patient sans chercher la prise de poids.",
    "applicabilyConditions": [
      {
        "condition": {
          "value": "(age_in_month >= 6) && (weight >= 3)",
          "variables": [
            "age_in_month",
            "weight"
          ]
        },
        "description": "Phase applicable aux enfants de 6 mois et plus pesant au moins 3 kg.",
        "varaibleExplaination": {
          "age_in_month": "Âge de l'enfant en mois (≥ 6 mois).",
          "weight": "Poids actuel de l'enfant en kilogrammes (≥ 3 kg)."
        }
      }
    ],
    "failureCriteria": [
      {
        "condition": {
          "value": "(days_in_phase >= 4) && (appetite_test_result == 'Negative')",
          "variables": [
            "days_in_phase",
            "appetite_test_result"
          ]
        },
        "description": "Absence de retour de l'appétit au 4ème jour.",
        "variablesExplanation": {
          "days_in_phase": "Nombre de jours que le patient a passés dans la phase actuelle (Phase 1).",
          "appetite_test_result": "Le résultat du dernier test d'appétit ('POSITIVE' ou 'NEGATIVE')."
        }
      },
      {
        "condition": {
          "value": "(days_in_phase >=4) && (observation_edema_godet_count >= initial_observation_edema_godet_count)",
          "variables": []
        },
        "description": "Absence de perte d'œdèmes au 4ème jour.",
        "variablesExplanation": {
          "days_in_phase": "Nombre de jours que le patient a passés dans la phase actuelle.",
          "initial_observation_edema_godet_count": "Le niveau d'œdème actuel du patient (0, 1, 2, ou 3).",
          "observation_edema_godet_count": "Le niveau d'œdème que le patient avait au début de cette phase."
        }
      },
      {
        "condition": {
          "value": "(days_in_phase>=10) && (clinical_edema == 1)",
          "variables": [
            "days_in_phase",
            "clinical_edema"
          ]
        },
        "description": "Présence d'œdèmes persistants au 10ème jour.",
        "variablesExplanation": {
          "days_in_phase": "Nombre de jours que le patient a passés dans la phase actuelle.",
          "clinical_edema": "Un booléen (vrai/faux) indiquant si le patient a des œdèmes."
        }
      }
    ],
    "transitionCriteria": [
      {
        "condition": {
          "value": "(appetite_test_result == 'Positive') && (complications_number == 0) && (observation_edema_godet_count <= 1)",
          "variables": [
            "appetite_test_result",
            "complications_number",
            "observation_edema_godet_count"
          ]
        },
        "description": "Retour de l'appétit, stabilité clinique et début de fonte des œdèmes.",
        "variablesExplanation": {
          "appetite_test_result": "Le résultat du dernier test d'appétit ('POSITIVE' ou 'NEGATIVE').",
          "complications_number": "Le nombre total de complications médicales graves et actives.",
          "observation_edema_godet_count": "Le niveau d'œdème actuel du patient. Le protocole demande un 'début de fonte', ce qui peut être interprété comme un niveau de + (1) ou 0."
        }
      }
    ],
    "recommendedTreatments": [
      {
        "identifier": "CNT_PHASE1::nutritional::F75",
        "applicabilityCondition": {
          "condition": {
            "value": "((age_in_month >= 6) && (weight >=3)) || ((age_in_month < 6) && (weight < 3) && (clinical_edema == 1))",
            "variables": [
              "age_in_month",
              "clinical_edema"
            ]
          },
          "descritpion": "F-75 indiqué en phase de stabilisation: ≥6 mois et ≥3 kg, ou <6 mois/<3 kg avec œdèmes.",
          "variableExplanation": {
            "age_in_month": "Âge en mois pour distinguer <6m vs ≥6m.",
            "clinical_edema": "Présence d'œdèmes (True/False)."
          }
        },
        "code": "f75",
        "type": "nutritional",
        "duration": {
          "type": "while_in_phase"
        }
      },
      {
        "identifier": "CNT_PHASE1::nutritional::F100::diluted",
